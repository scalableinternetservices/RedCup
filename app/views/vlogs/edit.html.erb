<div class="card" style="width: 80%;margin: AUTO;">
<h1>Editing Vlog</h1>
<div class="card-body">
<%= render 'form', vlog: @vlog %>
</div>
<br/>
<div class="card-footer bg-transparent border-success">
<%= link_to 'Show', @vlog, class:"btn btn-primary"%> 
<%= link_to 'Back', vlogs_path, class:"btn btn-primary" %>
<%= link_to 'Delete', @vlog, method: :delete, data: { confirm: 'Are you sure?'}, class: "btn btn-danger" %>
</div>
</div>

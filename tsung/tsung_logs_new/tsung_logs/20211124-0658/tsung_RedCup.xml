<?xml version="1.0"?><tsung loglevel="notice">
  <clients>
    <client host="localhost" maxusers="32768" use_controller_vm="true"/>
  </clients>

  <servers>
    <server host="page.eba-miy5wk4c.us-west-2.elasticbeanstalk.com" port="80" type="tcp"/>
  </servers>

  <load>
    <arrivalphase phase="1" duration="5" unit="second">
      <users arrivalrate="1" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="2" duration="5" unit="second">
      <users arrivalrate="2" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="3" duration="5" unit="second">
      <users arrivalrate="4" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="4" duration="5" unit="second">
      <users arrivalrate="8" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="5" duration="5" unit="second">
      <users arrivalrate="16" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="6" duration="5" unit="second">
      <users arrivalrate="32" unit="second"/>
    </arrivalphase>
  </load>

  <options>
    <option name="global_ack_timeout" value="2000"/>
  </options>
  <sessions>
    <session name="guest" type="ts_http" weight="30">
      <request>
        <http method="GET" url="/"/>
      </request>
      <request>
        <http method="GET" url="/vlogs/"/>
      </request>
      <request>
        <http method="GET" url="/vlogs/1/"/>
      </request>
      <thinktime value="5" random="true"/>
      <request>
        <http method="GET" url="/vlogs/2/"/>
      </request>
    </session>
    
    <session name="new_user" type="ts_http" weight="10">
      <request>
        <http method="GET" url="/"/>
      </request>
      <request>
        <http method="GET" url="/home/about/"/>
      </request>
      <thinktime value="5" random="true"/>
      <request>
        <dyn_variable name="authenticity_token"/>
        <http url="/users/sign_up/" method="GET" version="1.1"/>
      </request>
      <setdynvars sourcetype="random_string" length="8">
        <var name="rndstring1"/>
      </setdynvars>
      <request subst="true">
        <http content_type="application/x-www-form-urlencoded" contents="authenticity_token=%%_authenticity_token%%&amp;user%5Bemail%5D=%%_rndstring1%%%40mordor.com&amp;user%5Bpassword%5D=123456&amp;user%5Bpassword_confirmation%5D=123456&amp;commit=Sign%20up" method="POST" url="/users/" version="1.1">
        </http>
      </request>
      <request>
        <http method="GET" url="/vlogs/"/>
      </request>
      <request>
        <http method="GET" url="/vlogs?page=2"/>
      </request>
    </session>
    
    
    <session name="old_user" type="ts_http" weight="60">
      <request>
        <http method="GET" url="/"/>
      </request>
      <request>
        <dyn_variable name="authenticity_token"/>
        <http url="/users/sign_up/" method="GET" version="1.1"/>
      </request>
      <setdynvars sourcetype="random_string" length="8">
        <var name="rndstring"/>
      </setdynvars>
      <request subst="true">
        <http content_type="application/x-www-form-urlencoded" contents="authenticity_token=%%_authenticity_token%%&amp;user%5Bemail%5D=%%_rndstring%%%40mordor.com&amp;user%5Bpassword%5D=123456&amp;user%5Bpassword_confirmation%5D=123456&amp;commit=Sign%20up" method="POST" url="/users/" version="1.1">
        </http>
      </request>
      
      <request>
        <dyn_variable name="authenticity_token"/>
        <http url="/vlogs/new/" method="GET" version="1.1"/>
      </request>
      <request subst="true">
        <http content_type="application/x-www-form-urlencoded" contents="authenticity_token=%%_authenticity_token%%&amp;vlog%5Btitle%5D=HelloWorld&amp;vlog%5Bcontent%5D=123456&amp;vlog%5Buser_name%5D=Anonymous&amp;vlog%5Buser_id%5D=5&amp;commit=Create%20Vlog" method="POST" url="/vlogs/" version="1.1">
        </http>
      </request>
      
      <setdynvars sourcetype="random_number" start="1" end="100000">
        <var name="rndint"/>
      </setdynvars>
      <request>
        <dyn_variable name="authenticity_token"/>
        <http url="/vlogs/1/" method="GET" version="1.1"/>
      </request>
      <request subst="true">
        <http content_type="application/x-www-form-urlencoded" contents="authenticity_token=%%_authenticity_token%%&amp;commit=Like%20%E2%99%A5%EF%B8%8F" method="POST" url="/vlogs/1/likes/" version="1.1">
        </http>
      </request>
    
    </session>
    
    
  </sessions>
</tsung>
